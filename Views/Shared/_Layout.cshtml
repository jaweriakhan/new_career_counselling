                                 <a href="~/Home/assessment" class="fa-file  ">Assessment</a>
                              </li>

                        </ul>

                      }
                      else
                      {
                        <ul id="hornavmenu" class="nav navbar-nav">

                            <li>
                                <a href="~/Home/indexAdmin" class="fa-home ">Home</a>
                            </li>

                            <li>
                                <a href="~/Admin/manageInstitute" class="fa-university "> Manage Institute</a>
                             </li>
                            <li>
                                <a href="~/Admin/manage_degree" class="fa-university "> Manage Degree</a>
                            </li>
                            <li>
                                <a href="~/Admin/Institute_Degree" class="fa-university "> Link Institute & Degree</a>
                            </li>
                            <li>
                                <a href="~/Admin/manage_quiz" class="fa-university "> Manage Quiz</a>
                            </li>

                           </ul>
                      }


                    <ul id="hornavmenu" class="nav navbar-nav pull-right">

                        <li>

                            @if (Session["userName"] == null && Session["adminName"] == null)
                            {
                                <li>@Html.ActionLink("login", "login", "Home")</li>
                            }

                            
                            @if (Session["userName"] != null && Session["adminName"] == null)
                             {
                                
                                 <li>@Html.ActionLink( Session["FName"].ToString(), "index", "Home")</li>                                

                             }
                            else if (Session["userName"] == null && Session["adminName"] != null)
                            {
                                 <li>@Html.ActionLink(Session["adminName"].ToString(), "indexAdmin", "Admin")</li>
                              
                            }
                            @if (Session["userName"] != null || Session["adminName"] != null)
                            {
                                 <li>@Html.ActionLink("LogOut", "LogOut", "Home")</li>   
                            }
                            
                            




                        </ul>
</div>
                </div>
            </div>
        </div>
    </div>
