        <div class="container no-padding border-bottom">
            <div class="row">
                <div class="col-md-12 no-padding">
                    <div class="visible-lg">
                      @if (Session["adminName"] == null)
                      { 
                          <ul id="hornavmenu" class="nav navbar-nav">
                            
                            <li>
                                <a href="~/Home/index" class="fa-home ">Home</a>
                            </li>
                            <li>
                                <a href="~/Home/admission_notices" class="fa-adn " aria-hidden="true">Admission Notices</a>
                            </li>
                            <li>
                                <a href="~/Home/merit_calculator" class="fa-building-o ">Merit Calculator</a>
                            </li>
                            <!--<li>
                                <a href="~/Home/roadmap" class="fa-road ">Roadmap</a>
                            </li>
