    <script type="text/javascript" src="~/Scripts/jquery.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/scripts.js"></script>
    <!-- Isotope - Portfolio Sorting -->
