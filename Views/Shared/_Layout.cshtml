<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- Meta -->
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <!-- Favicon -->
    <link href="favicon.ico" rel="shortcut icon">
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="~/Content/bootstrap.css" rel="stylesheet">
    <!-- Template CSS -->
    <link rel="stylesheet" href="~/Content/animate.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/font-awesome.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/nexus.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/responsive.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/custom.css" rel="stylesheet">
